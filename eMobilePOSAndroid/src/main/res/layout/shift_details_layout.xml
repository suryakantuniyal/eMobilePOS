<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#FFFFFF"
android:orientation="vertical" >

<RelativeLayout
    android:id="@+id/headerContainer"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/blue_gradient_header_horizontal"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/headerTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/shift_details"
        android:gravity="center"
        android:background="@drawable/blue_gradient_header_horizontal"
        android:padding="10dp"
        android:textSize="@dimen/header_bar_text_size"
        android:textAppearance="@style/white_text_appearance"
        style="@style/text_shadow"/>

    <Button
        android:id="@+id/btnPrint"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/ordering_checkout_btn_height_size"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_margin="5dp"
        android:background="@drawable/blue_btn_selector"
        android:text="Print"
        android:textAppearance="@style/black_text_appearance"
        android:textSize="@dimen/ordering_checkout_btn_txt_size"
        android:paddingLeft="5dp"
        android:paddingRight="14dp" />
</RelativeLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/sales_clerk"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/salesClerktextView26"
                style="@style/black_text_appearance" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/begging_petty_cash"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/beginningPettyCashtextView26"
                style="@style/black_text_appearance" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/total_expenses"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/totalExpensestextView26"
                style="@style/black_text_appearance" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/ending_petty_cash"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/endingPettyCashtextView26"
                style="@style/black_text_appearance" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/total_transactions_cash"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/totalTransactionCashtextView26"
                style="@style/black_text_appearance" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/total_ending_cash"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/totalEndingCashtextView26"
                style="@style/black_text_appearance" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:text="@string/entered_close_amount"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView27"
                style="@style/black_text_appearance_unbold"
                android:layout_weight="1" />

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/enteredCloseAmounttextView26"
                style="@style/black_text_appearance" />
        </TableRow>


    </TableLayout>


</LinearLayout>